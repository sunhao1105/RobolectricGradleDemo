<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.douban.roboletricdemo.MainActivity"
    tools:ignore="MergeRootFrame" >

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/app_name" android:id="@+id/homeString"/>
    <Button
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="sqlite"
            android:id="@+id/goSqlite" android:layout_alignLeft="@+id/homeString" android:layout_alignParentTop="true"
            android:layout_marginTop="30dp"/>
    <Button
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="json"
            android:id="@+id/goJson"
            android:layout_alignLeft="@+id/homeString" android:layout_alignParentTop="true"
            android:layout_marginTop="90dp"/>
    <Button
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="intent"
            android:id="@+id/goIntent"
            android:layout_alignLeft="@+id/homeString" android:layout_alignParentTop="true"
            android:layout_marginTop="150dp"/>
    <EditText
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/giveString" android:layout_alignParentLeft="true" android:layout_marginLeft="116dp"
            android:layout_alignBottom="@+id/goIntent"/>

</FrameLayout>
